<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity Invoice.
    -->
    <changeSet id="20250515074245-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="client_id"
                                 baseTableName="invoice"
                                 constraintName="fk_invoice__client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"
                                 />

        <addForeignKeyConstraint baseColumnNames="partner_id"
                                 baseTableName="invoice"
                                 constraintName="fk_invoice__partner_id"
                                 referencedColumnNames="id"
                                 referencedTableName="partner"
                                 />
    </changeSet>
</databaseChangeLog>
